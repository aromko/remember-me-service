{
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ignore-pattern node_modules --ignore-pattern dist --ignore-pattern packages/types/src/index.ts --ignore-pattern \".cache\" --ignore-pattern public",
    "format": "prettier --write \"**/*\"",
    "test": "jest --config jest.config.js --runInBand && jest --config jest.node.config.js --runInBand",
    "test:coverage": "jest --config jest.config.js --coverage --coverageReporters html && jest --config jest.node.config.js --coverage --coverageReporters html",
    "test:ci": "jest --config jest.config.js --coverage --ci --no-cache && jest --config jest.node.config.js --coverage --ci --no-cache && pnpm merge-coverage",
    "precommit": "pnpm run lint && pnpm run format && pnpm run test && tsc",
    "merge-coverage": "istanbul-merge --out coverage/reports/merged/coverage-final.json coverage/reports/jsdom/coverage-final.json coverage/reports/node/coverage-final.json"
  },
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@marigold/components": "^5.6.0",
    "@marigold/eslint-config": "^0.4.10",
    "@marigold/jest-config": "^1.1.2",
    "@marigold/prettier-config": "^0.3.1",
    "@marigold/theme-b2b": "^24.0.0",
    "@testing-library/dom": "^9.3.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.1",
    "@types/node": "^18.16.15",
    "@types/react": "^18.2.7",
    "@types/react-dom": "^18.2.4",
    "bootstrap": "^5.2.3",
    "jest": "^29.5.0",
    "mongodb": "^5.5.0",
    "next": "13.4.10",
    "next-router-mock": "^0.9.3",
    "react": "^18.2.0",
    "react-bootstrap": "^2.7.4",
    "react-data-table-component": "^7.5.3",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "styled-components": "^6.0.0",
    "typescript": "^5.0.4"
  },
  "devDependencies": {
    "@swc/jest": "^0.2.26",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/testing-library__jest-dom": "^5.14.6",
    "husky": "^8.0.3",
    "istanbul-merge": "^2.0.0",
    "jest-fetch-mock": "^3.0.3",
    "mongodb-memory-server": "^8.13.0",
    "node-mocks-http": "^1.12.2"
  }
}
