    "@marigold/theme-b2b": "^23.0.0",
    "@testing-library/dom": "^9.2.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.1",
    "@types/node": "^18.16.3",
    "@types/react": "^18.2.1",
    "@types/react-dom": "^18.2.2",
    "bootstrap": "^5.2.3",
    "jest": "^29.5.0",
